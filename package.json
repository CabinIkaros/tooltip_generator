  },
  "homepage": "https://github.com/Shushishtok/tooltip_generator#readme",
  "devDependencies": {
    "@types/node": "^14.14.20",
    "fs-extra": "^9.0.1",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "link-module-alias": "^1.2.0",
    "node-watch": "^0.7.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.3"

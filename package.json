{
  "author": {
    "name": "Shush"
  },
  "bugs": {
    "url": "https://github.com/CabinIkaros/tooltip_generator/issues"
  },
  "bundleDependencies": false,
  "dependencies": {
    "link-module-alias": "^1.2.0",
    "node-watch": "^0.7.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.3",
    "valve-kv": "^1.1.0"
  },
  "deprecated": false,
  "description": "Code generator that makes creating tooltips for Dota 2 custom games much easier.",
  "devDependencies": {
    "@types/node": "^14.14.22",
    "fs-extra": "^9.1.0",
    "ts-node": "^10.9.2",
    "node-xlsx": "^0.23.0"
  },
  "homepage": "https://github.com/CabinIkaros/tooltip_generator#readme",
  "keywords": [
    "Dota",
    "Custom",
    "Game",
    "Tooltip",
    "Generator"
  ],
  "license": "ISC",
  "name": "@cabin-icarus/tooltip_generator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CabinIkaros/tooltip_generator.git"
  },
  "_moduleAliases": {
    "~resource": "resource",
    "~generator": "node_modules/@cabin-icarus/tooltip_generator"
  },
  "scripts": {
    "postinstall": "ts-node install.ts",
    "dev": "run-p dev:*",
    "dev:runlocalizationWatcher": "ts-node node_modules/~generator/fsWatcher.ts",
    "init": "link-module-alias && ts-node node_modules/~generator/init.ts"
  },
  "version": "1.7.7"
}
